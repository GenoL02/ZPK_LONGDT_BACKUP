projection;
strict ( 2 );
use draft;

define behavior for ZC_L_TASK //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Activate;
  use action Edit;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _Subject { create; with draft; }
}

define behavior for ZC_L_SUBJECT //alias <alias_name>
{
  use update;
  use delete;

  use association _Task { with draft; }
}